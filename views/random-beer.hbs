<div id="randomBeer">
    <img src="{{randomBeer.image_url}}" alt="">
    <div id="{{randomBeer.description}}">
        <h2>{{randomBeer.name}}</h2>
        <h3>{{randomBeer.tagline}}</h3>
        <h4>{{randomBeer.description}}</h4>
        <h4><b>Food Pairing</b></h4>
        <h4>
            <ul>
                {{#each randomBeer.food_pairing}}
                   <li>{{this}}</li>
                {{/each}}
            </ul>
        </h4>
        <h4><b>Brewers Tips</b> : {{randomBeer.brewers_tips}}</h4>
    </div>
</div>